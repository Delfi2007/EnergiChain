OPENAI_API_KEY=your_openai_api_key_here

# Node environment
NODE_ENV=development

# API Port
PORT=3001

# Rate limiting
MAX_REQUESTS_PER_MINUTE=60

# Model configurations
GPT_MODEL=gpt-4
EMBEDDING_MODEL=text-embedding-3-small

# RAG settings
RAG_TOP_K=3
RAG_MIN_SIMILARITY=0.7

# Paths
KNOWLEDGE_BASE_PATH=./rag/knowledge-base.json
EMBEDDINGS_CACHE_PATH=./rag/embeddings.json
